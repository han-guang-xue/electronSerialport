{
  "name": "client",
  "version": "1.1.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "rebuild": "electron-rebuild -f -w serialport",
    "builder": "electron-builder --win --x64",
    "packger": "electron-packager . --electron-version=10.1.1 --out=./out --download.mirrorOptions.mirror=https://npm.taobao.org/mirrors/electron/",
    "postinstall": "electron-builder install-app-deps",
    "more": "cnpm install && npm run rebuild && npm run builder"
  },
  "devDependencies": {
    "bootstrap": "^4.6.0",
    "electron": "^7.1.9",
    "electron-builder": "^22.7.0",
    "electron-rebuild": "^1.11.0",
    "electron-reloader": "^1.2.0",
    "jquery": "^3.5.1",
    "nodejs-websocket": "^1.7.2",
    "serialport": "^9.0.6",
    "electron-log": "4.3.0"
  },
  "dependencies": {
    "bootstrap": "^4.6.0",
    "jquery": "^3.5.1",
    "nodejs-websocket": "^1.7.2",
    "serialport": "^9.0.6",
    "electron-log": "4.3.0"
  },
  "build": {
    "appId": "com.vasen.serialport",
    "mac": {
      "target": [
        "dmg"
      ]
    },
    "win": {
      "target": [
        "nsis"
      ]
    },
    "nsis": {
      "oneClick": false,
      "guid": "xxxx",
      "perMachine": true,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true
    }
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0"
}
